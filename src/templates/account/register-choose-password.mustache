<div id="form_pw-tooltip" style="display: none">
    <p class="robustness">{{ password_robustness }} <span class="password_strategy_good">{{ good }}</span><span class="password_strategy_bad">{{ bad }}</span></p>
    {{# password_strategy_validators }}
    <p class="password_validator_msg_{{ key }}"><i class="icon-remove password_strategy_bad"></i> {{ description }}</p>
    {{/ password_strategy_validators }}
</div>

<script type="text/javascript">
    var password_validators = {{ json_password_strategy_keys }};
</script>
